{
  "name": "valencio/laravel-kit",
  "description": "My personal toolkit for Laravel 12.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Valencio\\LaravelKit\\": "src/"
    }
  },
  "authors": [
    {
      "name": "collin",
      "email": "collinai@163.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^12.0",
    "illuminate/http": "^12.0",
    "illuminate/filesystem": "^12.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Valencio\\LaravelKit\\Providers\\KitServiceProvider"
      ],
      "aliases": {
        "Kit": "Valencio\\LaravelKit\\Facades\\Kit"
      }
    }
  },
  "prefer-stable": true
}
